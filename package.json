{
  "name": "rclone",
  "version": "0.0.0-development",
  "description": "Decrypt filenames in the browser",
  "main": "dist/index.js",
  "module": "src/index.js",
  "author": "Fabrice Weinberg <fabrice@weinberg.me>",
  "license": "MIT",
  "files": ["dist", "src", "README.md"],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.2",
    "jest": "^22.1.4",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1",
    "stream-chunker": "^1.2.8",
    "stream-equal": "^1.1.0",
    "travis-deploy-once": "^4.3.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.0.0",
    "readable-stream": "^2.3.3",
    "scrypt-js": "^2.0.3",
    "through2": "^2.0.3",
    "tweetnacl": "^1.0.0"
  },
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main":
      "NODE_ENV=production babel --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename rclone.umd.js",
    "build:umd.min": "webpack -p --output-filename rclone.umd.min.js",
    "test": "jest",
    "dev": "webpack-dev-server",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["lcov", "html"]
  },
  "babel": {
    "presets": ["env"]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FWeinb/rclone-js.git"
  }
}
